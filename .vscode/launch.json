{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: train_MyhalCollision",
            "type": "python",
            "request": "launch",
            "program": "SOGM-3D-2D-Net/train_MyhalCollision.py",
            "console": "integratedTerminal"
        },
        {
            "name": "(gdb) Attach",
            "type": "cppdbg",
            "request": "attach",
            "program": "/usr/bin/python3", /* My virtual env */
            "processId": "${command:pickProcess}",
            "MIMode": "gdb",
            "setupCommands": [
                {
                    "description": "Enable pretty-printing for gdb",
                    "text": "-enable-pretty-printing",
                    "ignoreFailures": true
                }
            ]
        }
    ]
}